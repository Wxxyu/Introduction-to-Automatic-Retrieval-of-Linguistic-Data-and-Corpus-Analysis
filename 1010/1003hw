def addToInventory(inventory, addedItems):
  for item in addedItems:
    if item in inventory:
      inventory[item] += 1
    else:   
      inventory[item] = 1
  return inventory

inv = {'索仔': 1, '火把': 6, '銀角仔': 42, '刀仔': 1, '箭': 12}
dragonLoot = ['銀角仔', '刀仔', '銀角仔', '銀角仔', '紅寶石'] 

inv = addToInventory(inv, dragonLoot)

print(inv)
